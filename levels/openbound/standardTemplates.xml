<sburb desc="Standard Template Resources">
	<dependencies>
		<dependency>redBubbleText.xml</dependency>
	</dependencies>
	<assets>
		<asset name='preloaderBG' type='graphic'>backgrounds/load.png</asset>
		<asset name='volumeControl' type='graphic'>interface/volumeControl.png</asset>
		<asset name='saveControl' type='graphic'>interface/captcha.png</asset>
  		<asset name='helpControl' type='graphic'>interface/helpControl.png</asset>
  		<asset name='teleporterPad' type='graphic'>objects/teleporterpad.png</asset>
  		<asset name='teleportSheet' type='graphic'>effects/teleportSheet.png</asset>
  		
  		<asset name='chest1OpenSheet' type='graphic'>objects/chest1o.png</asset>
  		<asset name='chest1ClosedSheet' type='graphic'>objects/chest1.png</asset>
  		
  		<asset name='chest4OpenSheet' type='graphic'>objects/chest4o.png</asset>
  		<asset name='chest4ClosedSheet' type='graphic'>objects/chest4.png</asset>
  		<asset name='chest5OpenSheet' type='graphic'>objects/chest5o.png</asset>
  		<asset name='chest5ClosedSheet' type='graphic'>objects/chest5.png</asset>
  		<asset name='chest6OpenSheet' type='graphic'>objects/chest6o.png</asset>
  		<asset name='chest6ClosedSheet' type='graphic'>objects/chest6.png</asset>
  		

  		<asset name='redBubbleSheet' type='graphic'>objects/redbubble.png</asset>
	</assets>

	<classes>
	
		<sprite class='chest1' width='40' height='20' depthing='1' collidable='true'>
				<animation name='closed' sheet='chest1ClosedSheet' x='-48' y='-40' />
				<animation name='open' sheet='chest1OpenSheet' x='-48' y='-40' loopNum='0' followUp='opened'/>
				<animation name='opened' sheet='chest1OpenSheet' x='-48' y='-40' loopNum='0'/>
		</sprite>
		
		
		<sprite class='chest4' width='40' height='20' depthing='1' collidable='true'>
				<animation name='closed' sheet='chest4ClosedSheet' x='-48' y='-40' />
				<animation name='open' sheet='chest4OpenSheet' x='-48' y='-40'  loopNum='0' followUp='opened'/>
				<animation name='opened' sheet='chest4OpenSheet' x='-48' y='-40' loopNum='0'/>
		</sprite>
		
		<sprite class='chest5' width='40' height='20' depthing='1' collidable='true'>
				<animation name='closed' sheet='chest5ClosedSheet' x='-48' y='-40' />
				<animation name='open' sheet='chest5OpenSheet' x='-48' y='-40'  loopNum='0' followUp='opened'/>
				<animation name='opened' sheet='chest5OpenSheet' x='-48' y='-40' loopNum='0'/>
		</sprite>
		<sprite class='chest6' width='40' height='20' depthing='1' collidable='true'>
				<animation name='closed' sheet='chest6ClosedSheet' x='-48' y='-40' />
				<animation name='open' sheet='chest6OpenSheet' x='-48' y='-40'  loopNum='0' followUp='opened'/>
				<animation name='opened' sheet='chest6OpenSheet' x='-48' y='-40' loopNum='0'/>
		</sprite>
		
		
		<sprite class='teleporter' width='110' height='60'>
		  <animation sheet='teleporterPad' x='-66' y='-120'/>
	  	</sprite>
	  	
	  	<sprite class='redBubble' width='25' height='20' depthing='1' collidable='true'>
	  		<animation name='image' sheet='redBubbleSheet' x='-18' y='-60' />
			<action class='redBubbleText' name="talk" command='randomTalk'> </action>
		</sprite>
	  
	</classes>
	
	<effects>
  		<animation name='teleportEffect' x='-66' y='-110' sheet='teleportSheet' colSize='132' rowSize='165' length='6' frameInterval='2' loopNum='0'/>
	</effects>
</sburb>
